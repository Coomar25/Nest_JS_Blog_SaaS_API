    "nest-winston": "^1.9.4",
    "nestjs-seeder": "^0.3.2",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
